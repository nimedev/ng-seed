{
  "extends": "tslint-config-nimedev-ng",
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "nmd"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "nmd"
      ],
      "kebab-case"
    ]
  }
}